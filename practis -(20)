def matrix(satr,sotoon):
    result = []
    for i in range(satr):
        partial_result = []
        for i in range(sotoon):
            partial_result.append(1)
        result.append(partial_result)

    return result

m = matrix(6, 9)
for i in m:
    print(i)

m[2][4] = 0
print("-----")
for i in m:
    print(i)
